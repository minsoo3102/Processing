size(500,500); //종이의 사이즈
stroke(60,175,88); //선의 색
fill(50,70,100); //면의 색
print(250,250); //점의 좌표
line(250,100,250,400); //(x,y)(x,y)두 점을 이용한 선
rect(250,250,150,100); //(x,y)(w,h) w=넓이 h=높이 사각형
fill(10,20,130,50); //면의 색
ellipse(250,250,100,50); //타원
stroke(255); //선의 검은색
noFill();
rect(260,260,130,80); //마지막 숫자는 투명도
